{
  "name": "willdurand/geocoder-bundle",
  "description": "Integration of Geocoder into Symfony",
  "keywords": ["geocoding", "geocoder"],
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "William Durand",
      "email": "will+git@drnd.me"
    }
  ],
  "require": {
    "symfony/framework-bundle": "~3.0",
    "willdurand/geocoder": "4.0.1",
    "symfony/console": "~3.0"
  },
  "require-dev": {
    "php": ">=5.4",
    "symfony/yaml": "~3.0",
    "doctrine/cache": "~1.6",
    "doctrine/orm": "~2.5"
  },

  "autoload": {
    "psr-0": { "Bazinga\\Bundle\\GeocoderBundle": "" }
  },
  "target-dir": "Bazinga/Bundle/GeocoderBundle",
  "extra": {
    "branch-alias": {
      "dev-master": "4.1-dev"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/istvankis/Geocoder"
    }
  ]
}
